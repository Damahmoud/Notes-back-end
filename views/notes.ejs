<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('comm')%>
    <link rel="stylesheet" href="note.css">

</head>
<body>
    <div class="m"></div>
    <div class="container">

        <% if(notes.length > 0){ %>
            <% notes.forEach(e => {  %>
                <div class="note">
                    <h3><%= e.title %></h3>
                    <div class="note-content">
                        <p><%= e.body %></p>
                    </div>
                </div>
            <% }) %>    
        <% } else { %>
            <h1>There are no notes</h1>
        <% } %>
        
        
    </div>
    
</body>
</html>